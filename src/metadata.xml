<annotations version="1.0">
	<metadata 
		description="Robotlegs dependency injection"
		name="Inject"
		>
		<context name="variable"/>
		<context name="setter"/>
		<context name="method"/>
		<attribute 
			description="Name for a named injection"
			name="name"
			type="String"
			/>
	</metadata>
	<metadata 
		description="Robotlegs flag to automatically call this method after object construction (after injected properties are available)"
		name="PostConstruct"
		>
		<context name="method"/>
	</metadata>
</annotations>
